{
   "outFile": "./app.css",
   "colors": {
      "scss": "./dist/_colors.scss",
      "css": "./dist/colors.css",
      "source": "./_colors.scss",
      "type": "tailwind",
      "themeKeys": [
         "dark"
      ],
      "colorFormat": "oklch"
   },
   "media": {
      "breakpoints": {
         "xs": "320px",
         "sm": "360px",
         "md": "768px",
         "lg": "1024px",
         "xl": "1280px",
         "2xl": "1536px",
         "4k": "1920px"
      },
      "outDir": "./dist",
      "filename": "_mediaQuery.scss",
      "type": "tailwind",
      "useSass": false
   },
   "extra": {
      "font": {
         "sans": "'Inter, sans-serif'",
         "secondary": "Roboto, sans-serif"
      },
      "radius": {
         "default": "0.25rem",
         "sm": "0.125rem",
         "lg": "0.5rem",
         "full": "9999px"
      }
   },
   "watch": false
}